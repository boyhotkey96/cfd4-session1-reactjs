() {\n    let [count, setCount] = useState(0)\n    let [val, setVal] = useState(1)\n    let inputRef = useRef()\n    let countRef = useRef();\n\n\n\n    function setValue() {\n        // console.log(document.querySelector('.input-step').value)\n        setVal(parseInt(inputRef.current.value))\n    }\n\n    function setBg() {\n        console.log(countRef.current())\n        // countRef.current.style.background = 'red'\n    }\n\n    return (\n        <div>\n            <input type=\"number\" className=\"input-step\" ref={inputRef} />\n            <Count3Ref step={10} ref={countRef} />\n            <button onClick={setValue}>set value</button>\n            <button onClick={setBg}>set Background</button>\n        </div>\n    )\n}\n\n\nfunction Count({ step = 1 }, cRef) {\n\n    let [count, setCount] = useState(0);\n    function increment() {\n        count += step;\n        setCount(count)\n    }\n\n    function decrement() {\n        count -= step;\n        setCount(count)\n    }\n    return <div >\n        count: {count} <br />\n        <button ref={cRef} onClick={increment}>Increment</button>\n        <button onClick={decrement}>Decrement</button>\n    </div>\n}\n\n\nconst CountRef = React.forwardRef(Count)\n\n\n// Level 3\n\n\n\nfunction Count3({ step = 1 }, cRef) {\n\n\n    let [count, setCount] = useState(0);\n    let btnRef = useRef();\n    function increment() {\n        count += step;\n        setCount(count)\n    }\n\n    function decrement() {\n        count -= step;\n        setCount(count)\n    }\n\n    useImperativeHandle(\n        cRef,\n        () => {\n\n            return () => {\n                btnRef.current.style.background = 'green'\n            }\n        },\n    )\n\n    return <div >\n        count: {count} <br />\n        <button onClick={increment}>Increment</button>\n        <button ref={btnRef} onClick={decrement}>Decrement</button>\n    </div>\n}\n\n\nconst Count3Ref = React.forwardRef(Count3)","/Volumes/D/FRONT_END/CFD/session1/lab/cfd4-session1-reactjs/src/pages/register/reducer.js",["317","318","319"],"/Volumes/D/FRONT_END/CFD/session1/lab/cfd4-session1-reactjs/src/hook/formValidate.js",["320","321","322"],"import React, { useState } from \"react\";\n\nlet regexEmail = /^(([^<>()[\\]\\\\.,;:\\s@\"]+(\\.[^<>()[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/;\nlet regexUrlFacebook = /(?:(?:http|https):\\/\\/)?(?:www.)?facebook.com\\/(?:(?:\\w)*#!\\/)?(?:pages\\/)?(?:[?\\w\\-]*\\/)?(?:profile.php\\?id=(?=\\d.*))?([\\w\\-]*)?/;\nlet regexPhone = /\\(?([0-9]{3})\\)?([ .-]?)([0-9]{3})\\2([0-9]{4})/;\n\nexport default function useFormValidate(initialValue, validate) {\n  let [form, setForm] = useState(initialValue);\n\n  function inputChange(e) {\n    form[e.target.getAttribute(\"name\")] = e.target.value;\n    setForm({ ...form });\n  }\n\n  let [error, setError] = useState({});\n\n  function submit() {\n    let { rule, message } = validate;\n    let errorObject = {};\n\n    for (let i in rule) {\n      let r = rule[i];\n      console.log(r);\n\n      if (r.pattern) {\n        let pattern = r.pattern;\n\n        if (pattern === \"email\") pattern = regexEmail;\n        if (pattern === \"phone\") pattern = regexPhone;\n        if (pattern === \"url\") pattern = regexUrlFacebook;\n        // console.log(pattern)\n        if (!pattern.test(form[i])) {\n          errorObject[i] =\n            message?.[i]?.pattern || \"Không đúng định dạng yêu cầu\";\n        }\n      }\n      if (r.required) {\n        if (!form[i]) {\n          errorObject[i] =\n            message?.[i]?.required || \"Trường này không được để trống\";\n        }\n      }\n      if (r.min && form[i]?.length < r.min && form[i]?.length > 0) {\n        errorObject[i] = message?.[i]?.min || `Trường này dài hơn ${r.min} ký tự`\n      }\n      if (r.max && form[i]?.length > r.max) {\n        errorObject[i] = message?.[i]?.max || `Trường này dài hơn ${r.max} ký tự`\n      }\n    }\n\n    // for (let i in errorObject) {\n    //   document.querySelector(`[name=\"${i}\"]`).classList.add('error-input');\n    // }\n\n    setError(errorObject);\n    return errorObject;\n  }\n\n  return {\n    form,\n    inputChange,\n    error,\n    submit,\n  };\n}\n","/Volumes/D/FRONT_END/CFD/session1/lab/cfd4-session1-reactjs/src/hook/useAuth.js",[],{"ruleId":"323","replacedBy":"324"},{"ruleId":"325","replacedBy":"326"},{"ruleId":"327","severity":1,"message":"328","line":4,"column":8,"nodeType":"329","messageId":"330","endLine":4,"endColumn":14},{"ruleId":"327","severity":1,"message":"331","line":5,"column":8,"nodeType":"329","messageId":"330","endLine":5,"endColumn":16},{"ruleId":"332","severity":1,"message":"333","line":45,"column":17,"nodeType":"334","endLine":49,"endColumn":18},{"ruleId":"332","severity":1,"message":"333","line":16,"column":11,"nodeType":"334","endLine":16,"endColumn":40},{"ruleId":"332","severity":1,"message":"333","line":20,"column":13,"nodeType":"334","endLine":20,"endColumn":52},{"ruleId":"332","severity":1,"message":"333","line":23,"column":13,"nodeType":"334","endLine":23,"endColumn":54},{"ruleId":"332","severity":1,"message":"333","line":32,"column":13,"nodeType":"334","endLine":32,"endColumn":25},{"ruleId":"332","severity":1,"message":"333","line":35,"column":13,"nodeType":"334","endLine":35,"endColumn":25},{"ruleId":"332","severity":1,"message":"333","line":38,"column":13,"nodeType":"334","endLine":38,"endColumn":25},{"ruleId":"332","severity":1,"message":"333","line":41,"column":13,"nodeType":"334","endLine":41,"endColumn":25},{"ruleId":"332","severity":1,"message":"333","line":44,"column":13,"nodeType":"334","endLine":44,"endColumn":25},{"ruleId":"335","severity":1,"message":"336","line":19,"column":17,"nodeType":"334","endLine":19,"endColumn":51},{"ruleId":"332","severity":1,"message":"333","line":19,"column":17,"nodeType":"334","endLine":19,"endColumn":51},{"ruleId":"335","severity":1,"message":"336","line":20,