   {\n      username: \"\",\n      password: \"\",\n    },\n    {\n      rule: {\n        username: {\n          pattern: \"email\",\n          required: true,\n        },\n        password: {\n          required: true,\n          min: 6,\n          max: 32,\n        },\n      },\n      // message: {\n      //   username: {\n      //     pattern: \"Username không được để trống\",\n      //   },\n      //   password: {\n      //     min: \"Password phải dài hơn 6 ký tự\",\n      //     max: \"Password phải ngắn hơn 32 ký tự\",\n      //   },\n      // },\n    }\n  );\n\n  function btnSubmit(e) {\n    e.preventDefault();\n    let error = submit();\n    if (Object.keys(error).length === 0) {\n      setLoading(true);\n      setTimeout(() => {\n        setLoading(false);\n        alert(\"Đăng ký thành công\");\n      }, 1000);\n    }\n  }\n\n  return ReactDOM.createPortal(\n    <div\n      className=\"popup-form popup-login\"\n      ref={ref}\n      style={{ display: \"none\" }}\n    >\n      <div className=\"wrap\">\n        <form id=\"login\">\n          <div className=\"ct_login\" style={{ display: \"block\" }}>\n            <h2 className=\"title\">Đăng nhập</h2>\n            <input type=\"hidden\" className=\"url_post\" defaultValue />\n            <input\n              name=\"username\"\n              onChange={inputChange}\n              value={form.username}\n              type=\"text\"\n              placeholder=\"Email / Số điện thoại\"\n            />\n            {error.username && <p className=\"error-text\">{error.username}</p>}\n            <input\n              name=\"password\"\n              type=\"password\"\n              placeholder=\"Mật khẩu\"\n              onChange={inputChange}\n              value={form.password}\n            />\n            {error.password && <p className=\"error-text\">{error.password}</p>}\n            <p className=\"mess-error\" id=\"message_login\" />\n            <div className=\"remember\">\n              <label className=\"btn-remember\">\n                <div>\n                  <input type=\"checkbox\" />\n                </div>\n                <p>Nhớ mật khẩu</p>\n              </label>\n              <a\n                href=\"#\"\n                className=\"forget btn-open-popup\"\n                data-id=\"reset-password\"\n              >\n                Quên mật khẩu?\n              </a>\n            </div>\n            <div className=\"btn react main btn-login btn-register\" onClick={btnSubmit}>\n              đăng nhập\n              {loading && (\n                <CircularProgress size={20} style={{ marginLeft: 20 }} />\n              )}\n            </div>\n            <div className=\"text-register\" style={{ fontWeight: 700 }}>\n              <strong>Hoặc đăng nhập bằng</strong>\n            </div>\n            <div>\n              <div className=\"btn btn-login btn-google \" id=\"googleSignIn\">\n                <img src=\"/img/google.svg\" alt=\"\" />\n                Google\n              </div>\n              <p className=\"mess-error\" id=\"message_login_by_g\" />\n            </div>\n            <div className=\"close\" onClick={context.closePopupLogin}>\n              <img src=\"/img/close-icon.png\" alt=\"\" />\n            </div>\n          </div>\n        </form>\n      </div>\n    </div>,\n    document.getElementById(\"modal-root\")\n  );\n}\n\nexport default React.forwardRef(PopupLogin);\n","/Volumes/D/FRONT_END/CFD/session1/lab/cfd4-session1-reactjs/src/pages/profile/index.js",["291"],"/Volumes/D/FRONT_END/CFD/session1/lab/cfd4-session1-reactjs/src/pages/page404/index.js",[],"/Volumes/D/FRONT_END/CFD/session1/lab/cfd4-session1-reactjs/src/pages/profile/components/Course.js",["292","293"],"import React from \"react\";\n\nexport default function Course() {\n  return (\n    <div className=\"tab2\">\n      <div className=\"item\">\n        <div className=\"cover\">\n          <img src=\"/img/img3.png\" alt=\"\" />\n        </div>\n        <div className=\"